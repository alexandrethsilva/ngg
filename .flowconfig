[ignore]
.*/__tests__/.*
.*/babel/node_modules/.*
.*/babel-.*/node_modules/.*
.*/history/node_modules/.*
.*/jsxhint-.*/node_modules/.*
.*/react/node_modules/.*
.*/react-.*/node_modules/.*
.*/react-.*/node_modules/.*
.*/redux-react-router/node_modules/.*

[include]
node_modules/react/node_modules/fbjs/flow/include
node_modules/react/node_modules/fbjs/node_modules/promise
node_modules/react

[libs]
node_modules/react/node_modules/fbjs/flow/lib

[options]
module.system=haste
munge_underscores=true

suppress_type=$FlowIssue
suppress_type=$FlowFixMe
suppress_type=$FixMe

suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe\\($\\|[^(]\\|(\\(>=0\\.\\(1[0-6]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*www[a-z,_]*\\)?)\\)
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue\\((\\(>=0\\.\\(1[0-6]\\|[0-9]\\).[0-9]\\)? *\\(site=[a-z,_]*www[a-z,_]*\\)?)\\)? #[0-9]+
suppress_comment=\\(.\\|\n\\)*\\$FlowFixedInNextDeploy

[version]
0.16.0